noStroke();

var xPos = 200;
var yPos = 395;
var xPos2 = 393;
var yPos2 = 78;
var Size = 10;

draw = function() {
    
    background(29, 40, 115);
    fill(255, 242, 0);
    ellipse(xPos, yPos, Size, Size);
    
    
    fill(0, 85, 255);
    ellipse(xPos2, yPos2, Size, Size);
    
    
    yPos--;
    xPos2--;
    Size= Size+1;

    
    
};



